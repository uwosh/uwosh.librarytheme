<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en-US" lang="en-US"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="quills.app">

  <head>

    <metal:block define-macro="feed-links">
      <tal:block define="weblog_content python:view.getWeblogContentObject()">
         <metal:block metal:use-macro="context/quills_header_macros/feed-links-base"></metal:block>
      </tal:block>
    </metal:block>

    <metal:block define-macro="feed-links-base">
        <link
            rel="alternate"
            type="application/atom+xml"
            title="Atom feed"
            i18n:attributes="title"
            tal:attributes="href string:${weblog_content/absolute_url}/atom.xml" />

        <link
            rel="alternate"
            type="application/rss+xml"
            title="RSS 2.0 feed"
            i18n:attributes="title"
            tal:attributes="href string:${weblog_content/absolute_url}/rss.xml" />

        <link
            rel="alternate"
            type="application/rdf+xml"
            title="RDF 1.0 feed"
            i18n:attributes="title"
            tal:attributes="href string:${weblog_content/absolute_url}/feed.rdf" />

        <link
            rel="alternate"
            type="application/rdf+xml"
            title="RDF 1.1 feed"
            i18n:attributes="title"
            tal:attributes="href string:${weblog_content/absolute_url}/feed11.rdf" />
    </metal:block>

  </head>
  <body>
    
  </body>
</html>
